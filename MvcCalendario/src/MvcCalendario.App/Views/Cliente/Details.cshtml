@model MvcCalendario.App.ViewModels.ClienteViewModel

@{
    ViewData["Title"] = "Detalhe do Cliente";
}

<h4>@ViewData["Title"]</h4>


<div class="row">
    <h4>Informação somente leitura</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayFor(model => model.Nome)
        </dt>
        <dt class="col-sm-2">
            @Html.DisplayFor(model => model.Email)
        </dt>
        <dt class="col-sm-2">
            @Html.DisplayFor(model => model.CPF)
        </dt>
        <dt class="col-sm-2">
            @Html.DisplayFor(model => model.Grupo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataCadastro)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-dark" asp-action="Edit" asp-route-id="@Model.Id">Editar</a>
    <a class="btn btn-info" asp-action="Index">Voltar</a>
</div>

<partial name="_Enderecos" />
<partial name="_Contatos" />


@section Scripts {


<script>
    $(document).ready(function () {
        SetModal();
    });
</script>

}